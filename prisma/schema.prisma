}

model auction_document {
  ID             Int      @id @default(autoincrement())
  Title          String?  @db.VarChar(1000)
  Date           String   @db.VarChar(100)
  years          String   @db.VarChar(100)
  AD_ID          String?  @db.VarChar(10)
  ND_ID          String?  @db.VarChar(10)
