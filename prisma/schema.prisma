  Budget         Decimal  @db.Decimal(15, 2)
  NUMBER_PROJECT String   @db.VarChar(100)
  FIRSTNAME      String   @db.VarChar(300)
  TIMESTAMP      DateTime @default(now()) @db.Timestamp(0)
}

